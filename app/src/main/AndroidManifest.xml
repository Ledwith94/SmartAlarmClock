<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.conorwhyte.smartalarmclock">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
	<activity
            android:name="com.example.paulledwith.mapimplimentation.DistanceActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name=".AlarmReceiver" />

        <service
            android:name=".RingtonePlayingService"
            android:enabled="true" />

        <activity android:name=".PunisherActivity" />
        <activity android:name=".PuzzleActivity" />
        <activity android:name=".StopAlarmActivity" />
        <activity android:name=".ManagerHomeActivity" />
        <activity android:name=".CardListActivity"></activity>
	<meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="AIzaSyBNEagd68QNkTC3hwX_bCvD0b_NhAF_3xk" />
    </application>

</manifest>